Función N_17 <- ejercicio_17 (1)
	Definir num1, num2, num3, aux Como Entero
	Escribir 'ingrese 3 numeros' // defino la variables como entero
	Leer num1, num2, num3
	Si num1<num2 Entonces // leo y asigno tres numeros ;  1° a num1; 2° a num2; y 3° a num3
		aux <- num1 // si num2 es mayor a num1 le cambio los datos para que num1 sea el mayor
		num1 <- num2
		num2 <- aux
	FinSi
	Si num1<num3 Entonces
		aux <- num1 // si num3 es mayor a num1 le cambio los datos para que num1 sea el mayor
		num1 <- num3
		num3 <- aux
	FinSi
	Si num2<num3 Entonces
		aux <- num2 // si num3 es mayor a num2 le cambio los datos para que num2 sea el mayor
		num2 <- num3
		num3 <- aux
	FinSi
	Escribir num1, ' ', num2, ' ', num3
FinFunción


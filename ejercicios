	Escribir 'Ingrese un numero para multiplicarlo del 1 al 12 ' // escribo las variables como entero 
	Leer x // pido al usuario un numero 
	Para c<-1 Hasta 12 Con Paso 1 Hacer // le asigno ese numero a la variable x
		Escribir x, ' X ', c, ' = ', x*c // hago un bucle en el que imprima el numero y lo multiplique por 12 veces, 
	FinPara // por la variable c que va aumentando con un mas 1, teniendo una tabla de multiplicar al final 
FinFunción

Función N_36 <- ejercicio_36 (1)
	// números pares del 1 al 50
	Escribir 'Suma de los numeros pares del 1 al 50' // . Suma de números pares: Utiliza un bucle for para calcular la suma de los
	a <- 52 // uso un  ciclo "para"; para mas facilidad, ya que solo tienes que ingresar una variable 
	Para i<-2 Hasta 50 Con Paso 2 Hacer // y escribir hasta que punto quieres que llegue
		a <- a-2 // aplico ciclo "para" para hacer las sumas
		Escribir i, '+', a, '=', i+a
	FinPara
FinFunción

Función N_35 <- ejercicio_35 (1)
	// necesita y calcula el costo total. Si las horas son más de 10, aplica un descuento del 20 MOD 
	Definir descuento, costo, h, res Como Real // . Calculadora de costo de servicio: Pregunta al usuario cuántas horas de servicio
	descuento <- 0.20 // defino mis variables como reales 
	costo <- 2
	Escribir 'Cuantas horas de servicio nesesita una(1) hora le cuesta 2$ '
	Leer h // pregunto cuantas horas nesesita
	res <- h*costo // leo y le asigno el dato a la variable h
	Si h>10 Entonces
		Escribir 'Tiene un descuento del 20% por superar las 10 horas' // aplico condicional si Entonces, si es mayor a 10 se le descuenta el 20 MOD  
		res <- res-(res*descuento)
		Escribir 'El total es $', res
	SiNo
		Escribir 'El total es $', res // sino se le cobra el total normalmente
	FinSi
FinFunción

Función N_34 <- ejercicio_34 (1)
	// la cantidad de unidades de un producto que va a comprar y el precio unitario.Aplica descuentos por volumen de compra según las siguientes reglas:
	// 10-50 unidades: 5 MOD  de descuento 51-100 unidades: 10 MOD  de descuento Más de 100 unidades: 15 MOD  de descuento
	Definir unc, pc, pcf, des1, des2, des3 Como Real // Calculadora de descuento por volumen de compra: Permite al usuario ingresar
	des1 <- 0.05 // declaro mis variables Como Real
	des2 <- 0.10
	des3 <- 0.15
	Escribir 'Descuento por volumen de compra'
	Escribir 'Cuantas unidades de un producto va a comprar' // pregunto cuantas unidades va a llevar
	Leer unc
	Escribir 'Cual es el precio unitario' // leo y asigno en la vairble unc
	Leer pc // pregunto por el precio y se lo asigno a la variable pc
	pcf <- unc*pc
	Si unc>=10 Y unc<=50 Entonces // en caso de que los datos ingresados se menor a 10 se presentara directamente este resulatdo 
		Escribir 'Tiene un descuento del %5, Total $', pcf-(pcf*des1) // aplico 3 condicionales si Entonces 
	SiNo // en el caso de que esté entre el 10 y el 50 sele da el descuento de 5 MOD 
		Si unc>=51 Y unc<=100 Entonces
			Escribir 'Tiene un descuento del %10, Total $', pcf-(pcf*des2)
		SiNo // en el caso de que esté entre el 51 y el 100 sele da el descuento de 10 MOD 
			Si unc>100 Entonces
				Escribir 'Tiene un descuento del %15, Total $', pcf-(pcf*des3)
			SiNo // en el caso de que esté en el 101 para arriba sele da el descuento de 15 MOD 
				Escribir 'Su total es de $', pcf
			FinSi // si es menor a 10 no se le da ningun descuento
		FinSi
	FinSi
FinFunción

Función N_33 <- ejercicio_33 (1)
	// total de sus compras mensuales durante un año. Si el total es superior a $500,
	// aplica un descuento del 10 MOD  en la próxima compra.
	Definir cp, cp1, descuento, to Como Real // Calculadora de descuento por lealtad del cliente: Pide al usuario que ingrese el
	Definir res Como Cadena
	descuento <- 0.10 // defino las variables Como Entero y una Como Caracter
	Escribir 'Escriba el total de sus compras mensuales durante un año ($)'
	Leer cp // pregunto el total de sus compras de todo el año
	Si cp>500 Entonces // le asigno a cp el dato
		Escribir 'GENIAL! Tiene un descuento del %10 para su proxima compra' // uso 2 condicionales si Entonces en el primero si cp es mayor a 500 le doy el descuento
		Escribir 'Desea usar su descuento ahora (si, no)' // sino digo "a tu casa"
		Leer res
		Si res='si' Entonces // luego le pregunto si quiere usar su descuento ahora
			Escribir 'Escriba el total de la compra de hoy' // uso el segundo si Entonces, si su respuesta es (si) el codigo sigue 
			Leer cp1 // pregunto el total de la compra
			to <- cp1-(cp1*descuento) // le asigno el dato a la variable cp1
			Escribir 'su total es: $', to
		SiNo // escribo el resultado en consola
			Escribir 'No hay problema, su descuento seguira vigente hasta que lo canjee'
		FinSi // si el resultado es no escribo mensaje en consola
	SiNo
		Escribir 'Si su compras de todo el año superaban los $500 Recibia un descuento para su proxima compra'
	FinSi
FinFunción

Función N_32 <- ejercicio_32 (1)
	// usuario ingresar la distancia de envío y calcule el costo del envío. Si la distancia
	// es inferior a 50 km, el costo es de $10. Si la distancia es de 50 km o más, el costo es de $20.
	Definir km, co1, co2 Como Entero // Calculadora de envío con tarifas diferentes: Crea un programa que permita al
	Escribir 'Tarifas de envio, ingrese la distancia a recorrer para su envio(En Km)' // defino variables Como Entero
	Leer km // pregunto por los kilometros
	Si km<50 Entonces // leo el dato y le asigno a km
		Escribir 'El costo de envio es de $10' // utilizo condicional si Entonces , si los kilometros son menores a 50 el costo de envio es de $10
	SiNo
		Escribir 'El costo de envio es de $20'
	FinSi // si los kilometros son mayores o iguales a 50 el costo de envio es de $20
FinFunción // escribo el resultado

Función N_31 <- ejercicio_31 (1)
	// estado trabajando en una empresa y calcula su bono de antigüedad. Si ha
	// trabajado más de 5 años, otorga un bono del 5 MOD  sobre su salario.
	Definir ANOS, salario Como Entero // Descuento por antigüedad en la empresa: Pregunta al usuario cuántos años ha
	bono <- 0.05 // defino las variables Como Entero
	Escribir 'Cuantos años a estado trabajando en una empresa ' // le doy a bono un valor de 0.05 (para multiplicarlo y agregarlo al salario)
	Leer ANOS // pregunto "Cuantos años a estado trabajando"
	Escribir 'Escriba su salario (Mensual)' // escribo el dato en la varible años
	Leer salario // pregunto(Cuanto es su salario al mes)
	Si ANOS>=5 Entonces // escribo el dato en la variable salario, aplico condicional si entonces
		salario <- salario+(salario*bono) // si los años que trabajo son mayores o iguales a 5 entonces se la agrega el bono
		Escribir 'Tiene un bono del 5% sobre su salario $', salario
	SiNo // escribo el resultado
		Escribir 'No ha trabajado mas de 5 años (Sin cambios)' // si no trabajo 5 años o mas no se le suma el bono
	FinSi
FinFunción

Función N_30 <- ejercicio_30 (1)
	// usuario que ingrese su
	// salario anual y calcula el impuesto sobre la renta según las siguientes tasas:
	// Hasta De $10,001 a $20,000: 10 MOD  , menor a $10,000: 5 MOD  y
	// Más de $20,000: 15 MOD 
	Definir salario_anual, impuesto1, impuesto2, impuesto3, total Como Real // Calculadora de impuestos sobre el salario: Solicita al
	impuesto1 <- 0.05 // defino las variables Como Real
	impuesto2 <- 0.10 // le doy impuesto1, impuesto2, impuesto3 los valores de 5 MOD , 10 MOD , 15 MOD  dividiendolos para 100, para omitir un paso en el calculo
	impuesto3 <- 0.15
	Escribir 'ingrese su salario anual'
	Leer salario_anual
	Si salario_anual<=10000 Entonces // le pido al usuario que ingrese su salario anual  
		total <- (salario_anual*impuesto1) // si el salario es menor o igual a 10 mil aplico un impuesto del 5 MOD 
		Escribir ' su salario anual es menor a $10000 el impuesto a pagar es del 5%' // aplico la formula
		Escribir 'impuesto a pagar $', total
	SiNo // escribo el impuesto a pagar en consola
		Si (salario_anual>=10001) Y (salario_anual<=20000) Entonces
			total <- (salario_anual*impuesto2) // si el salario esta entre 10mil y 20mil aplico un impuesto del 10 MOD 
			Escribir ' su salario anual esta entre $10000 y $20000 el impuesto a pagar es del 10%' // aplico formula
			Escribir 'impuesto a pagar $', total
		SiNo // escribo el impuesto a pagar en consola
			Si salario_anual>20000 Entonces
				total <- (salario_anual*impuesto3) // si el salario es mayor a 20mil aplico un impuesto del 15 MOD 
				Escribir ' su salario anual es mayor a $20000 el impuesto a pagar es del 15%' // aplico formula
				Escribir 'impuesto a pagar $', total
			FinSi // escribo el resultado en consola
		FinSi
	FinSi
FinFunción

Función N_29 <- ejercicio_29 (1)
	// precio y la cantidad de varios artículos que está comprando. Calcula el total de
	// la compra y aplica un descuento del 10 MOD  si el total es mayor a cierta cantidad
	// (por ejemplo, $100)
	// asigno las variables como reales y el numero de articulos como entero
	Definir total, cantidad, subtotal, precio, descuento Como Real // Calculadora de compra con múltiples artículos: Permite al usuario ingresar el
	Definir numart Como Entero
	descuento <- 0.10
	total <- 0
	Escribir 'Numero de articulos llevará'
	Leer numart // se asigna el numero de articulos
	Para i<-1 Hasta numart Con Paso 1 Hacer
		Escribir 'Cuantos productos del articulo: ', i, ' lleva' // se hace un ciclo equivalente al numero de articulos
		Leer cantidad
		Escribir 'Cuanto cuesta el articulo: ', i // se toma la cantidad de productos 
		Leer precio
		subtotal <- cantidad*precio // se toma el precio de los productos
		total <- total+subtotal // se multiplica la cantidad de productos por su precio
	FinPara // durante el ciclo se van a almacenar todos los datos en el total
	Si total>100 Entonces
		total <- total-(total*descuento) // si el total es mayor a 100 se ejecuta acciones por verdadero
		Escribir 'Su total supera los $100 y por ello recive un descuento de 10%' // se le quita la decima parte del precio original
		Escribir 'valor final $', total
	SiNo
		Escribir 'Su total es $', total // si el total es inferior a 100 no se le ofrece el descuento
	FinSi
FinFunción

Función N_28 <- ejercicio_28 (1)
	// actual y el porcentaje de aumento que recibirá. Calcula y muestra el nuevo
	// salario después del aumento.
	Definir salario, aumento, valorfinal Como Real // Calculadora de sueldo con aumento: Pide al usuario que ingrese su salario
	Escribir 'Ingrese su salario' // defino las variables Como Real
	Leer salario // le pido al usuario que ingrese su salario al mes
	Escribir 'Ingrese el porcentaje de aumento' // le asigno el valor a salario
	Leer aumento // le pido al usuario un porcentaje para el aumento
	valorfinal <- salario+(salario*(aumento/100)) // le asigno el valor a la variable aumento
	Escribir 'su salario es ', valorfinal // aplico una formula 
FinFunción // imprimo el resultado en consola

Función N_27 <- ejercicio_27 (1)
	// de una factura y el porcentaje de impuestos aplicado. Luego, calcula y muestra
	// el monto total a pagar, incluyendo los impuestos.
	Definir factura, impuesto, valorfinal Como Real // Calculadora de factura con impuestos: Solicita al usuario que ingrese el total
	Escribir 'Ingrese el valor total de la factura' // defino las variables como reales
	Leer factura // le pido al usuario el valor de la factura
	Escribir 'Ingrese el valor de impuesto aplicado' // le asigno el valor a la variable factura
	Leer impuesto // le pido al usuario el impuesto correspondiente
	valorfinal <- factura+(factura*(impuesto/100)) // le aisgno el valor a la variable impuesto
	Escribir 'El valor total de la factura es ', factura, ' con el impuesto su total es de ', valorfinal // aplico una formula 
FinFunción // escribo los resultados en la consola 

Función N_26 <- ejercicio_26 (1)
	// usuario ingresar el precio de un artículo y un porcentaje de descuento. El
	// programa debe calcular y mostrar el precio final después del descuento.
	Definir precio, descuento, vf, preciof Como Real // Calculadora de precio con descuento: Crea un programa que permita a un
	Escribir 'Ingrese un precio' // defino las variables como reales 
	Leer precio // le pido al usuario que ingrese un valor
	Escribir 'Ingrese cual es el descuento que desea (1-100)' // lo asigno a la variable  precio
	Leer descuento // le pido al usuario qu e ingrese un descuento del 1 al 100
	vf <- ((precio*100)/descuento)/100 // lo asigno a la variable descuento
	precio <- precio-vf // aplico una formula medio rara 
	Escribir precio
FinFunción // escribo el resultado en la consola 


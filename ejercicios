FinFunción

Función N_40 <- ejercicio_40 (1)
	// número. Utiliza un bucle while para repetir la solicitud hasta que adivine correctamente.
	Definir num, x, intentos Como Entero // Adivina el número: Genera un número aleatorio y pide al usuario que adivine el
	num <- azar(10) // defino las variables Como Entero
	x <- -1
	Mientras x<>num Hacer // le doy a num un valor Aleatorio entre 0 y 10
		Escribir 'Adivina el numero entre 0 y 10' // Mientras Que el numero inggresado por el usuario sea diferente al numero aleatorio se va ha repetir el ciclo
		Leer x
	FinMientras
	Escribir 'Adivinaste'
FinFunción // si el usuario sale del ciclo escribir "Ganaste"

Función N_39 <- ejercicio_39 (1)
	// una palabra ingresada por el usuario.

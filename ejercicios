FinFunción // escribo el resutado en consola

Función N_38 <- ejercicio_38 (1)
	// palabra ingresada por el usuario.
	Definir frase Como Cadena // Contador de vocales: Utiliza un bucle while para contar el número de vocales en una
	Definir x, c, a Como Entero
	Escribir 'Escriba una frase'
	Leer frase
	x <- Longitud(frase)
	c <- 1
	a <- 0
	Mientras c<=x Hacer
		Según Subcadena(frase,c,c) Hacer
			'a', 'A', 'á':
				a <- a+1
			'e', 'E', 'é':
				a <- a+1
			'i', 'I', 'í':
				a <- a+1
			'o', 'O', 'ó':
				a <- a+1
			'u', 'U', 'ú':
				a <- a+1
		FinSegún
		c <- c+1
	FinMientras
	Escribir 'la frase tiene ', a, ' vocales'
FinFunción

Función N_37 <- ejercicio_37 (1)

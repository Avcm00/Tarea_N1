Función N_14 <- ejercicio_14 (1)
	// Calculadora simple: Crea una calculadora que realice operaciones básicas
	// como suma, resta, multiplicación y división, según la elección del usuario.
	Definir num1, num2, resultado Como Real
	Definir operador Como Entero // defino mis variables Como Real
	Escribir 'ingrese un numero'
	Leer num1
	Escribir 'ingrese el segundo numero' // leo y asigno el primer numero a num1
	Leer num2
	Escribir 'opciones' // leo y asigno el segundo numero a num2
	Escribir '1 = sumar' // le doy opciones al usuario segun el operador que desee emplear
	Escribir '2 = restar' // 1_sumar; 2_restar;3_multiplicar;4_dividir
	Escribir '3 = multiplicar'
	Escribir '4 = dividir'
	Leer operador
	Según operador Hacer // leo y asigno la respuesta en operador
		1:
			resultado <- num1+num2
		2: // si la respuesta es 1 se suman los 2 numeros
			resultado <- num1-num2
		3: // si la respuesta es 2 se restan los 2 numeros
			resultado <- num1*num2
		4: // si la respuesta es 3 se multiplican los 2 numeros
			resultado <- num1/num2
		De Otro Modo: // si la respuesta es 4 se dividen 
			Escribir 'opcion incorreta'
	FinSegún // si escribe un numero que no estaba entre las opciones= mensaje
	Escribir resultado
FinFunción

Función N_13 <- ejercicio_13 (1)
	// Número par o impar: Solicita al usuario que ingrese un número e indica si es
	// par o impar.
	Definir num1 Como Entero
	Escribir 'Escriba un numero' // declaro las variables Como Entero
	Leer num1
	Si num1 MOD 2=0 Entonces // leo y asigno el valor a num1
		Escribir 'El numero es par' // si el mod 2 de num es 0 es par
	SiNo
		Escribir 'El numero es impar'
	FinSi
	// num mod 2 = 0 ; es par ; 2 mod 2 = 0 
FinFunción

Función N_12 <- ejercicio_12 (1)
	// Área de un triángulo: Pide al usuario que ingrese la base y la altura de un triángulo,
	// luego calcula y muestra su área.
	Definir base, altura Como Entero
	Definir area Como Real
	Escribir 'Ingrese la base del triangulo' // defino las variables como enteros y reales
	Leer base // le pido al usuario la base del triangulo
	Escribir 'Ingrese la altura del triangulo' // le asigno el dato a base
	Leer altura // le pido al usuario la altura del triangulo
	area <- base*altura/2 // le asigno el dato a altura
	Escribir 'Area del triangulo ', area // realiso la operacion en la variable area
	// a= (base * altura)/2
	// a= baseAltura/2
FinFunción // imprimo el resulatdo en consola

Función N_11 <- ejercicio_11 (num1,num2,resultado)
	// entrada y muestre su suma.
	// declaro las variables
	Escribir 'Ingrese el primer numero' // 11. Suma de dos números: Escribe un programa que tome dos números como
	Leer num1 // le pido al  usuario un numero para sumar
	Escribir 'Ingrese el segundo numero' // asigno su respuesta a num1
	Leer num2 // le pido al  usuario otro numero para sumar
	resultado <- num1+num2 // asigno su respuesta a num1
	Escribir 'El resultado es ', resultado // hago la operacion en la variable resultado
	// num1 + num2
	// = num1+num2
FinFunción // escribo el resultado en consola

Función N_10 <- ejercicio_10 (1)
	// ((8 > 2)  O   O  (932 < 23) )  Y  4 == 2
	// 8 > 2 o 932 < 23) y 4 = 2
	// (verdadero o falso) y falso
	// verdadero y falso
	// verdadero
	Escribir 'Ejercicio_10'
	Escribir (8>2 O 932<23) Y 4=2
FinFunción // escribo el resultado en la consola

Función N_9 <- ejercicio_9 (1)
	// (48 < 2 * 3)  O   O  (2 * 7 < 12)
	// (48<2*3) o (2*7<12)
	// falso o falso
	// falso
	Escribir 'Ejercicio_9'
	Escribir (48<2*3) O (2*7<12)
FinFunción // escribo el resultado en la consola

Función N_8 <- ejercicio_8 (1)
	// 260 / 12 + 54  MOD  3 - 85  MOD  7 
	// 21.66+ 0 - 1
	// 20.66666667
	Escribir 'Ejercicio_8' // 260 / 12 + 54  MOD  3 ? 85  MOD  7
	Escribir 260/12+54 MOD 3-85 MOD 7
FinFunción // escribo el resultado en la consola

Función N_7 <- ejercicio_7 (1)
	Escribir 'Ejercicio_7' // 2 *(4 - 10 + 8)/2* 36 *(1/2)
	Escribir 2*(4-10+8)/2*36*(1/2)
	// 2 *(4 - 10 + 8) / 2* 36 *(1/2)
	// 2 *(2) / 2* 36 *(1/2)
	// 4/2 * 36 *(1/2)
	// 2* 36 *(1/2)
	// 72*(1/2)
	// 36
FinFunción // escribo el resultado en la consola

Función N_6 <- ejercicio_6 (1)
	Escribir 'Ejercicio_6' // (5 + 3 * 2) + 9 > 3 * 5 * 14  MOD  3
	Escribir (5+3*2)+9>3*5*14 MOD 3
	// (5 + 3 * 2) + 9 > 3 * 5 * 14 mod 3
	// ( 5 + 6 ) + 9 > 210 mod 3
	// 20>0
	// VERDADERO
FinFunción

Función N_5 <- ejercicio_5 (a,b,u)
	a <- 12 // Si a=12 y b=9, calcula el valor de u = b - a + 3 * a mod b.
	b <- 9
	u <- 0
	u <- b*a+3*a MOD b // les asigno un valor numerico de acuerdo a la operacion
	Escribir 'Ejercicio_5' // le doy el valor de u la operacion en general
	Escribir 9-12+3*12 MOD 9
	// u = 9 - 12 + 3 * 12 mod 9
	// u= 9 - 12 + 36 mod 9
	// u = 9 - 12 + 0
	// u = -3
FinFunción

Función N_4 <- ejercicio_4 (a,b,v)
	a <- 8 // Para a=8 y b=5, encuentra el valor de v = 2 * b + a div 2 + 4 * b mod a.
	b <- 5
	v <- 0
	v <- 2*b+a+trunc(2+4)*b MOD a // les asigno un valor numerico de acuerdo a la operacion
	Escribir 'Ejercicio_4' // le doy el valor de v la operacion en general
	Escribir 2*5+trunc(8+2)+4*5 MOD 8
	// v = 2 * b + a div 2 + 4 * b mod a.
	// 2 * 5 + trunc(8 + 2) +4 * 5 mod 8
	// 10 + 10 + 20 mod 8
	// 20+4 
	// 24
FinFunción

Función N_3 <- ejercicio_3 (a,b,w)
	a <- 6
	b <- 2
	w <- 0
	w <- a-b+2*a MOD b // les asigno un valor numerico de acuerdo a la operacion
	Escribir 'Ejercicio_3' // le doy el valor de w la operacion en general
	Escribir 6-2+2*6 MOD 2
	// w = 6 - 2 + 2 * 6 mod 2
	// 6 - 2 + 12 mod 2
	// 6- 2 + 0
	// 4
FinFunción

Función N_2 <- ejercicio_2 (a,b,z)
	a <- 10
	b <- 4
	z <- 0
	z <- a*b+3 MOD a+b // les asigno un valor numerico de acuerdo a la operacion
	Escribir 'Ejercicio_2' // le doy el valor de z la operacion en general
	Escribir 10*4+3 MOD 10+4
	// z = 10 * 4 + 3 mod 10 + 4
	// 40 + 3 + 4
	// 47
FinFunción

Función N_1 <- ejercicio_1(a,b,x)
	
	// Dado a=3 y b=7, encuentra el valor de y = 2 * a + b - a mod 3
	// defino mis variables Como Entero
	a <- 3
	b <- 7
	x <- 0
	x <- 2*a+b-a MOD 3 // les asigno un valor numerico de acuerdo a la operacion
	Escribir 'Ejercicio_1' // le doy el valor de x la operacion en general
	Escribir 2*3+7-3 MOD 3
	// y = 2 * 3 + 7 - 3 mod 3
	// 6 + 7 - 0
	// 13
FinFunción

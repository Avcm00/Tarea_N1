Función N_24 <- ejercicio_24 (1)
	// representa el domingo, 2 el lunes, 3 el martes, y así sucesivamente. Luego,
	// utiliza una estructura switch para mostrar el nombre del día de la semana
	// correspondiente al número ingresado.
	Definir dia Como Entero // Día de la semana: Pide al usuario que ingrese un número del 1 al 7, donde 1
	Definir diaf Como Cadena
	Escribir 'Ingrese un numero del 1 al 7' // defino las variables como caracter y  como entero 
	Leer dia // pido al usuario un nuemero entre el 1 y el 7
	Si dia=1 Entonces // lo asigno a la variable dia 
		diaf <- 'Domingo' // si dia es 1 entonces escribir es domingo
	SiNo
		Si dia=2 Entonces // sino aplico condiacional
			diaf <- 'Lunes' // si el dia es 2 entonces escribir es lunes
		SiNo
			Si dia=3 Entonces // sino aplico condiacional
				diaf <- 'Martes' // si el dia es 3 escribir es martes
			SiNo
				Si dia=4 Entonces // sino aplico condiacional
					diaf <- 'Miercoles' // si el dia es 4 Escribir es miercoles
				SiNo
					Si dia=5 Entonces // sino aplico condiacional
						diaf <- 'Jueves' // si el dia es 5 Escribir es jueves
					SiNo
						Si dia=6 Entonces // sino aplico condiacional 
							diaf <- 'Viernes' // si el dia es 6 Escribir es viernes
						SiNo
							Si dia=7 Entonces // sino aplico condiacional
								diaf <- 'Sabado' // si el dia es 7 Escribir es sabado
							SiNo
								Escribir 'Numero incorrecto'
							FinSi
						FinSi
					FinSi
				FinSi
			FinSi
		FinSi
	FinSi
	Escribir diaf
FinFunción

Función N_23 <- ejercicio_23 (1)
	// un número de día del mes (por ejemplo, del 1 al 31) y verifica si ese día
	// pertenece a la primera quincena (días 1-15) o a la segunda quincena (días 16-31).
	Definir dia Como Entero // Día del mes con respecto a la segunda quincena: Solicita al usuario que ingrese
	Escribir 'Ingrese un número de día del mes del 1 al 31' // defino las variables como entero
	Leer dia // pregunto al usuario por un dia  del mes
	Si (dia>=1 Y dia<=15) Entonces // le asigno el dato a la variable dia
		Escribir dia, ' Pertenece a la primera quincena' // aplico condicional si entonces, si está entre el 1 y el 15 decir pertenece a la primera quincena
	SiNo
		Si (dia>=16 Y dia<=31) Entonces // sino , aplico otro si entonces
			Escribir dia, ' Pertenece a la segunda quincena' // si el dia esta entre el 16 y el 31  decir pertenece a la segunda quincena 
		SiNo
			Escribir 'No se puede '
		FinSi
	FinSi
FinFunción

Función N_22 <- ejercicio_22 (1)
	// determina su signo zodiacal. Puedes usar una serie de declaraciones if para
	// comparar las fechas ingresadas con las fechas límite de cada signo zodiacal.			
	Definir dia, mes, c Como Entero // Signo zodiacal: Pide al usuario que ingrese su mes y día de nacimiento, luego
	Definir signo Como Cadena // defino las variables como entero y una como caracter
	Escribir 'Ingresa Día de Nacimiento'
	Leer dia // pregunto al usario su dia de nacimiento y se lo asigno a la variable dia
	Escribir 'Ingresa Mes de Nacimiento'
	Leer mes // pregunto al usario su mes nacimiento y se lo asigmo a la variable mes
	c <- 0
	Si (mes=1 Y (dia>=21 Y dia<=31)) O (mes=2 Y (dia<=19)) Entonces
		signo <- 'Acuario' // si el mes es igual a 1 o igual a 2 y entre el 21 de enero y 19 de febrero 
		c <- 1 // escribir tu sigmo es acuario
	SiNo
		Si (mes=2 Y (dia>=19 Y dia<=29)) O (mes=3 Y (dia<=20)) Entonces
			signo <- 'Piscis' // si el mes es igual a 2 o igual a 3 y entre el 19 de febrero y 20 de marzo
			c <- 1 // escribir tu signo es piscis
		SiNo
			Si (mes=3 Y (dia>=21 Y dia<=31)) O (mes=4 Y (dia<=19)) Entonces
				signo <- 'Aries' // si el mes es igual a 3 o igual a 4 entre el 21 de marzo  y el 19 de abril
				c <- 1 // escribir tu signo es Aries
			SiNo
				Si (mes=4 Y (dia>=20 Y dia<=30)) O (mes=5 Y (dia<=20)) Entonces
					signo <- 'Tauro' // si el mes es igual a 4 o igual a 5 entre el 20 de abril y el 20 mayo
					c <- 1 // escribir tu signo es tauro
				SiNo
					Si (mes=5 Y (dia>=21 Y dia<=31)) O (mes=6 Y (dia<=20)) Entonces
						signo <- 'Géminis' // si el mes es igual a 5 o igual a 6 entre el 21 de mayo y el 20 de junio
						c <- 1 // escribir tu signo es geminis
					SiNo
						Si (mes=6 Y (dia>=21 Y dia<=30)) O (mes=7 Y (dia<=22)) Entonces
							signo <- 'Cáncer' // si el mes es igual a 6 o igual a 7 entre el 21 de junio y el 22 de julio
							c <- 1 // escribir tu signo es cancer; tienes cancer
						SiNo
							Si (mes=7 Y (dia>=23 Y dia<=31)) O (mes=8 Y (dia<=22)) Entonces
								signo <- 'Leo' // si el mes es igual a 7 o igual a 8 entre el 23 de julio y el 22 de agosto 
								c <- 1 // escribir tu signo es Leo
							SiNo
								Si (mes=8 Y (dia>=23 Y dia<=31)) O (mes=9 Y (dia<=22)) Entonces
									signo <- 'Virgo' // si el mes es igual a 8 o igual a 9 entre el 23 de agosto y el 22 de septiembre 
									c <- 1 // escribir tu signo es Virgo
								SiNo
									Si (mes=9 Y (dia>=23 Y dia<=30)) O (mes=10 Y (dia<=22)) Entonces
										signo <- 'Libra' // si el mes es igual a 9 igual a 10 entre el  23 de septiembre y el 22 de octubre
										c <- 1 // escribir tu signo es libra
									SiNo
										Si (mes=10 Y (dia>=23 Y dia<=31)) O (mes=11 Y (dia<=21)) Entonces
											signo <- 'Escorpio' // si el mes es igual a 10 o igual a 11 entre el 23 de octubre y el 21 de noviembre 
											c <- 1 // escribir tu signo es escorpio
										SiNo
											Si (mes=11 Y (dia>=22 Y dia<=30)) O (mes=12 Y (dia<=21)) Entonces
												signo <- 'Sagitario' // si el mes es igual a 11 o igual a 12 entre el 22 de noviembre  y el 21 de diciembre 
												c <- 1 // escribir tu signo es sagitario
											SiNo
												Si (mes=12 Y (dia>=22 Y dia<=31)) O (mes=1 Y (dia<=18)) Entonces
													signo <- 'Capricornio' // si el mes es igual a 12 o igual a 1 entre el 22 de diciembre y el 18 de enero  
													c <- 1 // escribir tu signo es capricornio
												SiNo
													Escribir 'la fecha ingresada es incorrecta'
												FinSi
											FinSi
										FinSi
									FinSi
								FinSi
							FinSi
						FinSi
					FinSi
				FinSi
			FinSi
		FinSi
	FinSi
	Si c=1 Entonces
		Escribir 'Tu signo zodiacal es ', signo
	SiNo
		Escribir 'la fecha es incorrecta'
	FinSi
FinFunción

Función N_21 <- ejercicio_21 (1)
	// Un año bisiesto es divisible por 4, pero no por 100, a menos que también sea
	// divisible por 400.
	Definir ANO Como Entero // Año bisiesto: Solicita al usuario un año y determina si es un año bisiesto o no.
	Escribir 'Ingrese Año para determinar si es bisiesto o no' // defino la variable Como Entero
	Leer ANO // pregunto un año y le asigno a la variable año
	Si ANO MOD 4=0 Entonces
		Escribir 'Año bisiesto' // aplico condicional si  entonces 
	SiNo // si a años lo divido entre 4 y su mod es 0 entonces escribir es año bisiesto 
		Escribir 'Año no bisiesto'
	FinSi // si el mod es cualquier otro numero escribir el año no es bisiesto
FinFunción

